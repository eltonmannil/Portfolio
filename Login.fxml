<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Login">
    <Label fx:id="loginHeader" alignment="CENTER" layoutX="184.0" layoutY="38.0" prefHeight="63.0" prefWidth="193.0"
           text="Login">
        <font>
            <Font name="System Bold" size="32.0"/>
        </font>
    </Label>
    <Label fx:id="usernameLabel" layoutX="69.0" layoutY="145.0" prefHeight="36.0" prefWidth="150.0" text="Username">
        <font>
            <Font size="21.0"/>
        </font>
    </Label>
    <Label fx:id="passwordLabel" layoutX="69.0" layoutY="200.0" prefHeight="36.0" prefWidth="150.0" text="Password">
        <font>
            <Font size="21.0"/>
        </font>
    </Label>
    <TextField fx:id="usernameField" layoutX="281.0" layoutY="145.0" prefHeight="36.0" prefWidth="242.0"/>
    <PasswordField fx:id="passwordField" layoutX="281.0" layoutY="196.0" prefHeight="36.0" prefWidth="242.0"/>
    <Label fx:id="timeZoneLabel" layoutX="69.0" layoutY="295.0" prefHeight="36.0" prefWidth="186.0" text="Time Zone">
        <font>
            <Font size="21.0"/>
        </font>
    </Label>
    <Label fx:id="timeZoneDisplayLabel" disable="true" layoutX="281.0" layoutY="295.0" prefHeight="36.0"
           prefWidth="242.0">
        <font>
            <Font size="21.0"/>
        </font>
    </Label>
    <Button fx:id="loginActionButton" layoutX="281.0" layoutY="258.0" mnemonicParsing="false" onAction="#loginAction"
            prefHeight="28.0" prefWidth="110.0" text="Login"/>
    <Button fx:id="exitActionButton" layoutX="410.0" layoutY="258.0" mnemonicParsing="false" onAction="#exitAction"
            prefHeight="28.0" prefWidth="110.0" text="Exit"/>
</AnchorPane>
